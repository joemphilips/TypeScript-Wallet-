{
  "name": "@joemphilips/walletts",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all protoc && tsc -w",
    "test": "run-s test:*",
    "test:main": "nyc ava-ts",
    "test:security": "nsp check",
    "pretest": "npm-run-all protoc",
    "protoc":
      "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:generated --ts_out=service=true:generated -I ./proto proto/*.proto",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier ./**/*.ts ./**/**/*.ts --write"
  },
  "bin": {
    "walletts": ".//bin/walletts"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": ["prettier --write", "git add"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awilix": "^3.0.6",
    "axios": "^0.18.0",
    "blockchain-proxy":
      "git+https://github.com/joemphilips/blockchain-proxy.git#master",
    "bunyan": "^1.8.12",
    "commander": "^2.14.1",
    "eventemitter2": "^5.0.1",
    "grpc": "^1.9.1",
    "grpc-web-client": "^0.5.0",
    "ini": "^1.3.5",
    "inquirer": "^5.1.0",
    "ts-protoc-gen": "^0.4.0"
  },
  "devDependencies": {
    "@types/bitcoinjs-lib": "^3.3.1",
    "@types/bunyan": "^1.8.4",
    "@types/commander": "^2.12.2",
    "@types/eventemitter2": "^4.1.0",
    "@types/glob": "^5.0.35",
    "@types/ini": "^1.3.29",
    "@types/inquirer": "^0.0.37",
    "ava": "^1.0.0-beta.3",
    "ava-ts": "^0.24.2",
    "husky": "^0.15.0-rc.9",
    "lint-staged": "^7.0.0",
    "mali": "^0.7.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "nyc": "^11.5.0",
    "prettier": "^1.11.1",
    "ts-node": "^5.0.1",
    "tsc": "^1.20150623.0",
    "typescript": "^2.8.0-insiders.20180127"
  }
}
